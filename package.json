{
	"name": "cantata-tools",
	"displayName": "Cantata Tools",
	"description": "Tools to help create and mod Cantata",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:cantata-tools.interactable.new",
		"onCommand:cantata-tools.terrain.new",
		"onCustomEditor:cantata-tools.interactable",
		"onCustomEditor:catCustoms.terrain"
	],
	"main": "./out/extension.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "cantata-tools.interactable",
				"displayName": "Cantata Interactable",
				"selector": [
					{
						"filenamePattern": "*.cinteractable"
					}
				]
			},
			{
				"viewType": "cantata-tools.terrain",
				"displayName": "Cantata Terrain",
				"selector": [
					{
						"filenamePattern": "*.cterrain"
					}
				]
			}
		],
		"commands": [
			{
				"command": "cantata-tools.interactable.new",
				"title": "Create new Cantata Interactable",
				"category": "Cantata Tools"
			},
			{
				"command": "cantata-tools.interactable.new",
				"title": "Create new Cantata Interactable",
				"category": "Cantata Tools"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"eslint": "^7.6.0",
		"@typescript-eslint/eslint-plugin": "^3.8.0",
		"@typescript-eslint/parser": "^3.8.0",
		"glob": "^7.1.6",
		"mocha": "^8.0.1",
		"typescript": "^3.8.3",
		"vscode-test": "^1.4.0"
	}
}
